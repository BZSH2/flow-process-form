{
  "name": "flow-process-form",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "format": "prettier --write --experimental-cli src/",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix",
    "lint:eslint": "eslint . --fix --cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx,.vue",
    "lint:style": "stylelint --fix  **/*.{css,less,scss.vue}",
    "preview-https": "serve dist",
    "test": "vitest",
    "tsc": "vue-tsc --noEmit ",
    "typecheck": "node --max-old-space-size=4096 node_modules/vue-tsc/bin/vue-tsc.js --noEmit",
    "reinstall": "rimraf node_modules && npm run bootstrap",
    "lint:lint-staged": "lint-staged",
    "lint:all": "npm run lint:oxlint && npm run lint",
    "cz": "git pull && git add . && git-cz",
    "prepare": "husky",
    "openI18n": "esno openI18n/index.ts",
    "openApi": "esno openApi/index.ts",
    "changelog": "changelogen --output CHANGELOG.md",
    "changelog:release": "changelogen --release --output CHANGELOG.md"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "axios": "^1.13.4",
    "element-plus": "^2.13.2",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.23",
    "pinia": "^3.0.4",
    "pinyin-pro": "^3.28.0",
    "vue": "^3.5.27",
    "vue-i18n": "^11.2.8",
    "vue-router": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@intlify/unplugin-vue-i18n": "^11.0.3",
    "@playwright/test": "^1.58.1",
    "@tsconfig/node24": "^24.0.4",
    "@types/js-cookie": "3.0.6",
    "@types/jsdom": "^27.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.9",
    "@types/nunjucks": "^3.2.6",
    "@types/reserved-words": "^0.1.4",
    "@typescript-eslint/parser": "^8.54.0",
    "@unocss/eslint-config": "^66.6.0",
    "@unocss/eslint-plugin": "^66.6.0",
    "@unocss/vite": "^66.6.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vitest/eslint-plugin": "^1.6.6",
    "@vue/compiler-sfc": "^3.5.27",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "changelogen": "^0.6.0",
    "cz-git": "^1.12.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-format": "^1.3.1",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-oxlint": "~1.42.0",
    "eslint-plugin-playwright": "^2.5.1",
    "eslint-plugin-vue": "~10.7.0",
    "esno": "^4.8.0",
    "git-cz": "^4.9.0",
    "glob": "^13.0.1",
    "google-translate-api-x": "^10.7.2",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "jsonc-eslint-parser": "^2.4.2",
    "lint-staged": "^16.2.7",
    "lodash-es": "^4.17.23",
    "npm-run-all2": "^8.0.4",
    "nunjucks": "^3.2.4",
    "openapi3-ts": "^4.5.0",
    "oxlint": "~1.42.0",
    "prettier": "3.8.1",
    "reserved-words": "^0.1.2",
    "sass-embedded": "^1.97.3",
    "stylelint": "^17.1.0",
    "stylelint-config-recess-order": "^7.6.0",
    "stylelint-config-standard": "^40.0.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-scss": "^7.0.0",
    "typescript": "~5.9.3",
    "unocss": "^66.6.0",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^7.3.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.18",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.4"
  }
}
