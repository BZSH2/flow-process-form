                    "type": "string",
                    "description": "账单地址的第一行。",
                    "writeOnly": true
                  },
                  "address_line2": {
                    "type": "string",
                    "description": "账单地址的第二行。",
                    "writeOnly": true
                  },
                  "address_city": {
                    "type": "string",
                    "description": "账单地址所在城市。"
                  },
                  "address_country": {
                    "type": "string",
                    "description": "账单地址所在国家（两字母国家码）。"
                  },
                  "address_post_code": {
                    "type": "string",
                    "description": "账单地址邮编。"
                  }
                },
                "required": [
                  "name",
                  "number",
                  "cvc",
                  "exp_month",
                  "exp_year",
                  "address_country"
                ]
              },
              {
                "title": "银行账户",
                "description": "用于支付的银行账户。必须支持收款货币。",
                "type": "object",
                "properties": {
                  "object": {
                    "const": "bank_account",
                    "type": "string",
                    "description": "表示该对象为 'bank_account'。"
                  },
                  "name": {
                    "type": "string",
                    "description": "账户持有人姓名。"
                  },
                  "number": {
                    "type": "string",
                    "description": "银行账户号码（字符串）。"
                  },
                  "sort_code": {
                    "type": "string",
                    "description": "银行账户的 Sort Code（6 位字符串）。"
                  },
                  "account_type": {
                    "enum": [
                      "individual",
                      "company"
                    ],
                    "type": "string",
                    "description": "账户持有实体类型。"
                  },
                  "bank_name": {
                    "type": "string",
                    "description": "与路由号关联的银行名称。",
                    "examples": [
                      "Starling Bank"
                    ]
                  },
                  "country": {
                    "type": "string",
                    "description": "两位国家代码（ISO 3166-1 alpha-2）。"
                  }
                },
                "required": [
                  "name",
                  "number",
                  "account_type",
                  "bank_name",
                  "country"
                ]
              }
            ]
          },
          "status": {
            "description": "支付状态：pending、succeeded 或 failed 之一。",
            "type": "string",
            "enum": [
              "pending",
              "succeeded",
              "failed"
            ],
            "readOnly": true
          }
