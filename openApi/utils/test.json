                    "description": "卡持人姓名（与卡面一致）。",
                    "examples": [
                      "Francis Bourgeois"
                    ]
                  },
                  "number": {
                    "type": "string",
                    "description": "卡号字符串，不含分隔符（读取时为脱敏格式）。",
                    "examples": [
                      "4242424242424242"
                    ]
                  },
                  "cvc": {
                    "type": "string",
                    "description": "卡片安全码，通常为卡背后三/四位。",
                    "minLength": 3,
                    "maxLength": 4,
                    "writeOnly": true,
                    "examples": [
                      "123"
                    ]
                  },
                  "exp_month": {
                    "type": "integer",
                    "format": "int64",
                    "description": "卡到期月份，两位数字。",
                    "examples": [
                      12
                    ]
                  },
                  "exp_year": {
                    "type": "integer",
                    "format": "int64",
                    "description": "卡到期年份，四位数字。",
                    "examples": [
                      2025
                    ]
                  },
