      "OrderPayment": {
        "description": "订单支付信息。",
        "type": "object",
        "properties": {
          "id": {
            "description": "支付唯一标识。用于其他对象关联本支付。",
            "type": "string",
            "format": "uuid",
            "readOnly": true
          },
          "amount": {
            "description": "支付金额。待收款的正十进制数。",
