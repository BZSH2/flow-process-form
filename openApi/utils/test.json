  "openapi": "3.1.0",
  "info": {
    "title": "storeAPI",
    "description": "管理宠物、订单、购买、用户等宠物店业务操作。",
    "version": "1.0.0"
  },
  "tags": [
    {
      "name": "Payments"
    },
    {
      "name": "store"
    }
  ],
  "paths": {
    "/orders/{orderId}/payment": {
      "post": {
        "summary": "支付订单",
        "deprecated": false,
        "description": "支付是对订单发起支付的尝试，确认支付后用户可以获取其票据。",
        "operationId": "create-order-payment",
        "tags": [
          "Payments"
        ],
        "parameters": [
          {
            "name": "orderId",
            "in": "path",
            "description": "",
            "required": true,
            "example": "123456",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OrderPayment"
              },
              "examples": {
                "Card": {
                  "value": {
                    "amount": 49.99,
                    "currency": "gbp",
                    "source": {
                      "object": "card",
                      "name": "J. Doe",
                      "number": "4242424242424242",
                      "cvc": "123",
                      "exp_month": 12,
                      "exp_year": 2025,
                      "address_line1": "123 Fake Street",
                      "address_line2": "4th Floor",
                      "address_city": "London",
                      "address_country": "gb",
                      "address_post_code": "N12 9XX"
                    }
                  },
                  "summary": "银行卡支付"
                },
                "Bank": {
                  "value": {
                    "amount": 100.5,
                    "currency": "gbp",
                    "source": {
                      "object": "bank_account",
                      "name": "J. Doe",
                      "number": "00012345",
                      "sort_code": "000123",
                      "account_type": "individual",
                      "bank_name": "Starling Bank",
                      "country": "gb"
                    }
                  },
                  "summary": "银行账户支付"
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "支付成功",
            "content": {
              "application/json": {
                "schema": {
