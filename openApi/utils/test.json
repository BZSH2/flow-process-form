          "id": {
            "type": "integer",
            "format": "int64",
            "description": "订单唯一标识。"
          },
          "petId": {
            "type": "integer",
            "format": "int64",
            "description": "所订购的宠物编号。"
          },
          "quantity": {
            "type": "integer",
            "format": "int32",
            "description": "订单数量。"
          },
          "shipDate": {
            "type": "string",
            "format": "date-time",
            "description": "订单预计发货日期。"
          },
          "status": {
            "type": "string",
            "description": "订单状态",
            "enum": [
              "placed",
              "approved",
              "delivered"
            ]
          },
          "complete": {
            "type": "boolean",
            "description": "订单是否已完成。"
          }
        },
        "xml": {
          "name": "Order"
        }
      }
